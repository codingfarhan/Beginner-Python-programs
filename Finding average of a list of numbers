print('welcome to the mean calculator')
k=int(input("how many elements does your individual series contain?"))
j=[]
sum=0
print("please enter the elements")
for num in range(k):
	 x=int(input())
	 sum+=int(x)
	 j.append(x)
     
print("average of elements of the list is:  ", sum/k)
input('enter any key to exit')
